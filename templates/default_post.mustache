<div class="post">
	{{# metadata.title }}
	<div class="post-title">
		<h2 class="post-title-header">{{ metadata.title }}</h2>
	</div>
	{{/ metadata.title }}

	{{# content }}
	<div class="post-body">
		{{ content }}
	</div>
	{{/ content }}


	{{# metadata.tags }}
	<ul>
		{{# metadata.tags.values }}<li>{{ . }}</li>{{/ metadata.tags.values }}
	</ul>
	{{/ metadata.tags }}




</div>
